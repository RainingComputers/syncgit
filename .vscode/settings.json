        "autoattribute",
        "autoclass",
        "automethod",
        "genindex",
        "getenv",
        "gitcmd",
        "maxdepth",
        "modindex",
        "pylint",
        "quickstart",
        "subp",
        "syncgit's",
        "toctree"
